{
  "rewrites": [
    { "source": "/", "destination": "/index.html" },
    { "source": "/ar", "destination": "/index.html" },
    { "source": "/ar.html", "destination": "/index.html" }
  ],
  "redirects": [
    { "source": "/ar.html", "destination": "/", "permanent": true }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}

