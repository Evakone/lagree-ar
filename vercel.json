{
    "version": 2,
    "name": "lagree-ar",
    "rewrites": [
      { "source": "/", "destination": "/landing.html" },
      { "source": "/web-ar", "destination": "/index.html" },
      { "source": "/marker-ar", "destination": "/mindar.html" },
      { "source": "/advanced-ar", "destination": "/mindar-advanced.html" },
      { "source": "/debug", "destination": "/mindar-debug.html" }
    ],
    "headers": [
      {
        "source": "/assets/(.*)\\.glb",
        "headers": [
          { "key": "Content-Type", "value": "model/gltf-binary" },
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/js/(.*)\\.wasm",
        "headers": [
          { "key": "Content-Type", "value": "application/wasm" },
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
  }