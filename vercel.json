{
    "version": 2,
    "name": "lagree-ar",
    "rewrites": [
      { "source": "/", "destination": "/index.html" }
    ],
    "headers": [
      {
        "source": "/assets/(.*)\\.glb",
        "headers": [
          { "key": "Content-Type", "value": "model/gltf-binary" },
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      },
      {
        "source": "/js/(.*)\\.wasm",
        "headers": [
          { "key": "Content-Type", "value": "application/wasm" },
          { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
        ]
      }
    ]
  }